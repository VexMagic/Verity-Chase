    Backgrounds: []
    GainedClues: []
    GainedLocations: []
    GainedNotes: []
    Movements: []
    Music: {fileID: 8300000, guid: d3c0a8d9ca7ef4a4ebee2964eaf21a06, type: 3}
  - TalkingCharacter: 12
    Text: "What other information could you possibly have against me?\r"
    Backgrounds: []
    GainedClues: []
    GainedLocations: []
    GainedNotes: []
    Movements: []
    Music: {fileID: 0}
  - TalkingCharacter: 10
