    Backgrounds: []
    GainedClues: []
    GainedLocations: []
    GainedNotes: []
    Movements: []
    Music: {fileID: 0}
  - TalkingCharacter: 10
    Text: "...\r"
    Backgrounds: []
    GainedClues: []
    GainedLocations: []
    GainedNotes: []
    Movements: []
    Music: {fileID: 8300000, guid: 1fb1dc1d620c9554392f868815ab72b5, type: 3}
