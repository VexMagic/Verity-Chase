    Backgrounds: []
    GainedClues: []
    GainedLocations: []
    GainedNotes: []
    Movements: []
    Music: {fileID: 0}
  - TalkingCharacter: 9
    Text: "Because that's exactly what I used to do, and still sometimes do today.
      \r"
    Backgrounds: []
    GainedClues: []
    GainedLocations: []
    GainedNotes: []
    Movements: []
    Music: {fileID: 0}
  - TalkingCharacter: 10
