    GainedClues: []
    GainedLocations: []
    GainedNotes: []
    Movements: []
    Music: {fileID: 8300000, guid: d3c0a8d9ca7ef4a4ebee2964eaf21a06, type: 3}
  - TalkingCharacter: 4
    Text: "What! That's crazy! I-I mean... That's preposterous! \r"
    Backgrounds: []
    GainedClues: []
    GainedLocations: []
    GainedNotes: []
    Movements: []
    Music: {fileID: 0}
  - TalkingCharacter: 2
    Text: "Mr. Murphy was looking for the true identity of The Catalyst.\r"
    Backgrounds: []
    GainedClues: []
    GainedLocations: []
    GainedNotes: []
    Movements: []
    Music: {fileID: 0}
  - TalkingCharacter: 2
