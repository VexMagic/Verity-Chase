    Backgrounds: []
    GainedClues: []
    GainedLocations: []
    GainedNotes: []
    Movements: []
    Music: {fileID: 0}
  - TalkingCharacter: 4
    Text: "B-but I... I had no idea he was doing that. I promise!\r"
    Backgrounds: []
    GainedClues: []
    GainedLocations: []
    GainedNotes: []
    Movements: []
    Music: {fileID: 0}
